{
    "name": "NodeHyperCMS",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "build": "babel src -d dist",
        "server": "babel-node ./src/server.js",
        "bun_server": "bun run build && bun ./dist/server.js",
        "bun_start": "pm2 start bun -- run server",
        "bun_stop": "pm2 stop bun",
        "bun_restart": "pm2 restart bun",
        "npm_server": "npm run build && npm ./dist/server.js",
        "npm_start": "pm2 start npm -- run server",
        "npm_stop": "pm2 stop npm",
        "npm_restart": "pm2 restart npm",
        "start": "node start.js start",
        "restart": "node start.js restart",
        "stop": "node start.js stop",
        "test": "artillery run artillery.yaml"
    },
    "author": "urobot2011",
